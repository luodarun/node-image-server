{
    "apps": [
      {
      "name": "self-image-server",
      "script": "index.js",
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "error_file": "log/node-app.error.log",
      "out_file": "/dev/null",
      "min_uptime": "200s",
      "max_restarts": 10,
      "max_memory_restart": "100M",
      "cron_restart": "1 0 * * *",
      "watch": false,
      "merge_logs": true,
      "instances": 2,
      "exec_interpreter": "node",
      "exec_mode": "cluster",
      "autorestart": false,
      "vizion": false
    }]
  }
  